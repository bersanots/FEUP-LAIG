        <primitive id="patch">
            <patch npointsU="3" npointsV="3" npartsU="15" npartsV="2">
                <controlpoint xx="-1.7" yy="2" zz="-0.3" />
                <controlpoint xx="-1.7" yy="2" zz="-0.3" />
                <controlpoint xx="-1.7" yy="2" zz="-0.3" />
                <controlpoint xx="1.5" yy="1.5" zz="0" />
                <controlpoint xx="1.5" yy="2" zz="1.5" />
                <controlpoint xx="1.5" yy="2.5" zz="0" />
